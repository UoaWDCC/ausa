{
  "name": "project_ausa",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "lint": "eslint",
    "lint-and-fix": "eslint . --fix",
    "lint-all": "eslint .",
    "prettier": "prettier . --write",
    "prettier:ci": "prettier . --check",
    "dev-client": "yarn workspace client dev",
    "dev-server": "yarn workspace server start",
    "storybook": "yarn workspace client storybook",
    "vsc-setup": "code --install-extension dbaeumer.vscode-eslint && code --install-extension esbenp.prettier-vscode && code install-extension bradlc.vscode-tailwindcss",
    "build": "yarn workspace client build && yarn workspace server build"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "jest": "^29.7.0",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.30.1"
  },
  "engines": {
    "node": ">=22",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@4.9.1"
}
